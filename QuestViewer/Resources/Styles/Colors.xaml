<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="LightGreyBrush" Color="#f5f5f5" />

    <SolidColorBrush x:Key="BlueUltraSoftColorBrush" Color="#FFB2C8DF" />
    <SolidColorBrush x:Key="BlueSoftColorBrush" Color="#FF6798ca" />
    <SolidColorBrush x:Key="BlueColorBrush" Color="#FF3E79B4" />
    
    <SolidColorBrush x:Key="BlackPrimaryColorBrush" Color="#000000" />
    <SolidColorBrush x:Key="WhitePrimaryColorBrush" Color="#FFFFFF" />

    <SolidColorBrush x:Key="RedPrimaryBrush" Color="#EF4444" />
    <SolidColorBrush x:Key="RedExtraBrush" Color="#DC2626" />
    <SolidColorBrush x:Key="RedMiddleBrush" Color="#F87171" />

    <SolidColorBrush x:Key="GrayPrimaryBrush" Color="#71717A" />
    <SolidColorBrush x:Key="LightGrayPrimaryBrush" Color="#BBBBBB" />
    <SolidColorBrush x:Key="GrayExtraBrush" Color="#52525B" />

    <!--  Primary button  -->

    <Color x:Key="PrimaryButtonForegroundColor">#FFFFFF</Color>
    <Color x:Key="PrimaryButtonBackgroundColor">#FF3B82F6</Color>
    <Color x:Key="PrimaryButtonBorderColor">#FF3B82F6</Color>

    <Color x:Key="PrimaryButtonHoverForegroundColor">#FFFFFF</Color>
    <Color x:Key="PrimaryButtonHoverBackgroundColor">#5291f7</Color>
    <Color x:Key="PrimaryButtonHoverBorderColor">#5291f7</Color>

    <Color x:Key="PrimaryButtonActiveForegroundColor">#FFFFFF</Color>
    <Color x:Key="PrimaryButtonActiveBackgroundColor">#0C356A</Color>
    <Color x:Key="PrimaryButtonActiveBorderColor">#0C356A</Color>

    <Color x:Key="PrimaryButtonDisableForegroundColor">#71717A</Color>
    <Color x:Key="PrimaryButtonDisableBackgroundColor">#FFE2E2E4</Color>
    <Color x:Key="PrimaryButtonDisableBorderColor">#FFE2E2E4</Color>

    <SolidColorBrush x:Key="PrimaryButtonForeground" Color="{StaticResource PrimaryButtonForegroundColor}" />
    <SolidColorBrush x:Key="PrimaryButtonBackground" Color="{StaticResource PrimaryButtonBackgroundColor}"  />
    <SolidColorBrush x:Key="PrimaryButtonBorder" Color="{StaticResource PrimaryButtonBorderColor}"  />

    <SolidColorBrush x:Key="PrimaryButtonHoverForeground" Color="{StaticResource PrimaryButtonHoverForegroundColor}" />
    <SolidColorBrush x:Key="PrimaryButtonHoverBackground" Color="{StaticResource PrimaryButtonHoverBackgroundColor}"  />
    <SolidColorBrush x:Key="PrimaryButtonHoverBorder" Color="{StaticResource PrimaryButtonHoverBorderColor}" />

    <SolidColorBrush x:Key="PrimaryButtonActiveForeground" Color="{StaticResource PrimaryButtonActiveForegroundColor}" />
    <SolidColorBrush x:Key="PrimaryButtonActiveBackground" Color="{StaticResource PrimaryButtonActiveBackgroundColor}"  />
    <SolidColorBrush x:Key="PrimaryButtonActiveBorder" Color="{StaticResource PrimaryButtonActiveBorderColor}"  />

    <SolidColorBrush x:Key="PrimaryButtonDisableForeground" Color="{StaticResource PrimaryButtonDisableForegroundColor}"  />
    <SolidColorBrush x:Key="PrimaryButtonDisableBackground" Color="{StaticResource PrimaryButtonDisableBackgroundColor}"  />
    <SolidColorBrush x:Key="PrimaryButtonDisableBorder" Color="{StaticResource PrimaryButtonDisableBorderColor}"  />
    <!--  Blue button end  -->

    <!--  SecondaryFirst button start  -->

    <Color x:Key="SecondaryFirstButtonForegroundColor">#FF3B82F6</Color>
    <Color x:Key="SecondaryFirstButtonBackgroundColor">#00000000</Color>
    <Color x:Key="SecondaryFirstButtonBorderColor">#FF3B82F6</Color>

    <Color x:Key="SecondaryFirstButtonHoverForegroundColor">#FF3B82F6</Color>
    <Color x:Key="SecondaryFirstButtonHoverBackgroundColor">#AADDF3F5</Color>
    <Color x:Key="SecondaryFirstButtonHoverBorderColor">#FF3B82F6</Color>

    <Color x:Key="SecondaryFirstButtonActiveForegroundColor">#FF3B82F6</Color>
    <Color x:Key="SecondaryFirstButtonActiveBackgroundColor">#C3D4D4</Color>
    <Color x:Key="SecondaryFirstButtonActiveBorderColor">#FF3B82F6</Color>

    <Color x:Key="SecondaryFirstButtonDisableForegroundColor">#6E85B7</Color>
    <Color x:Key="SecondaryFirstButtonDisableBackgroundColor">#AADDF3F5</Color>
    <Color x:Key="SecondaryFirstButtonDisableBorderColor">#6E85B7</Color>

    <SolidColorBrush x:Key="SecondaryFirstButtonForeground" Color="{StaticResource SecondaryFirstButtonForegroundColor}" />
    <SolidColorBrush x:Key="SecondaryFirstButtonBackground" Color="{StaticResource SecondaryFirstButtonBackgroundColor}"  />
    <SolidColorBrush x:Key="SecondaryFirstButtonBorder" Color="{StaticResource SecondaryFirstButtonBorderColor}"  />

    <SolidColorBrush x:Key="SecondaryFirstButtonHoverForeground" Color="{StaticResource SecondaryFirstButtonHoverForegroundColor}" />
    <SolidColorBrush x:Key="SecondaryFirstButtonHoverBackground" Color="{StaticResource SecondaryFirstButtonHoverBackgroundColor}"  />
    <SolidColorBrush x:Key="SecondaryFirstButtonHoverBorder" Color="{StaticResource SecondaryFirstButtonHoverBorderColor}" />

    <SolidColorBrush x:Key="SecondaryFirstButtonActiveForeground" Color="{StaticResource SecondaryFirstButtonActiveForegroundColor}" />
    <SolidColorBrush x:Key="SecondaryFirstButtonActiveBackground" Color="{StaticResource SecondaryFirstButtonActiveBackgroundColor}"  />
    <SolidColorBrush x:Key="SecondaryFirstButtonActiveBorder" Color="{StaticResource SecondaryFirstButtonActiveBorderColor}"  />

    <SolidColorBrush x:Key="SecondaryFirstButtonDisableForeground" Color="{StaticResource SecondaryFirstButtonDisableForegroundColor}"  />
    <SolidColorBrush x:Key="SecondaryFirstButtonDisableBackground" Color="{StaticResource SecondaryFirstButtonDisableBackgroundColor}"  />
    <SolidColorBrush x:Key="SecondaryFirstButtonDisableBorder" Color="{StaticResource SecondaryFirstButtonDisableBorderColor}"  />

    <!--  SecondaryFirst button end  -->

    <!--  SecondaryTwo button start  -->

    <Color x:Key="SecondaryTwoButtonForegroundColor">#FFEF4444</Color>
    <Color x:Key="SecondaryTwoButtonBackgroundColor">#00000000</Color>
    <Color x:Key="SecondaryTwoButtonBorderColor">#FFEF4444</Color>

    <Color x:Key="SecondaryTwoButtonHoverForegroundColor">#FFEF4444</Color>
    <Color x:Key="SecondaryTwoButtonHoverBackgroundColor">#FECACA</Color>
    <Color x:Key="SecondaryTwoButtonHoverBorderColor">#FFEF4444</Color>

    <Color x:Key="SecondaryTwoButtonActiveForegroundColor">#FFEF4444</Color>
    <Color x:Key="SecondaryTwoButtonActiveBackgroundColor">#FF9F9F</Color>
    <Color x:Key="SecondaryTwoButtonActiveBorderColor">#FFEF4444</Color>

    <Color x:Key="SecondaryTwoButtonDisableForegroundColor">#FECACA</Color>
    <Color x:Key="SecondaryTwoButtonDisableBackgroundColor">#55ECCFD1</Color>
    <Color x:Key="SecondaryTwoButtonDisableBorderColor">#FECACA</Color>

    <SolidColorBrush x:Key="SecondaryTwoButtonForeground" Color="{StaticResource SecondaryTwoButtonForegroundColor}" />
    <SolidColorBrush x:Key="SecondaryTwoButtonBackground" Color="{StaticResource SecondaryTwoButtonBackgroundColor}"  />
    <SolidColorBrush x:Key="SecondaryTwoButtonBorder" Color="{StaticResource SecondaryTwoButtonBorderColor}"  />

    <SolidColorBrush x:Key="SecondaryTwoButtonHoverForeground" Color="{StaticResource SecondaryTwoButtonHoverForegroundColor}" />
    <SolidColorBrush x:Key="SecondaryTwoButtonHoverBackground" Color="{StaticResource SecondaryTwoButtonHoverBackgroundColor}"  />
    <SolidColorBrush x:Key="SecondaryTwoButtonHoverBorder" Color="{StaticResource SecondaryTwoButtonHoverBorderColor}" />

    <SolidColorBrush x:Key="SecondaryTwoButtonActiveForeground" Color="{StaticResource SecondaryTwoButtonActiveForegroundColor}" />
    <SolidColorBrush x:Key="SecondaryTwoButtonActiveBackground" Color="{StaticResource SecondaryTwoButtonActiveBackgroundColor}"  />
    <SolidColorBrush x:Key="SecondaryTwoButtonActiveBorder" Color="{StaticResource SecondaryTwoButtonActiveBorderColor}"  />

    <SolidColorBrush x:Key="SecondaryTwoButtonDisableForeground" Color="{StaticResource SecondaryTwoButtonDisableForegroundColor}"  />
    <SolidColorBrush x:Key="SecondaryTwoButtonDisableBackground" Color="{StaticResource SecondaryTwoButtonDisableBackgroundColor}"  />
    <SolidColorBrush x:Key="SecondaryTwoButtonDisableBorder" Color="{StaticResource SecondaryTwoButtonDisableBorderColor}"  />

    <!--  SecondaryFirst button end  -->

    <!--  Gray button  -->
    <SolidColorBrush x:Key="GrayButtonForeground" Color="#52525B" />
    <SolidColorBrush x:Key="GrayButtonBackground" Color="#E3E3E3" />
    <SolidColorBrush x:Key="GrayButtonBorder" Color="#71717A" />

    <SolidColorBrush x:Key="GrayButtonHoverForeground" Color="#52525B" />
    <SolidColorBrush x:Key="GrayButtonHoverBackground" Color="#EAEAEA" />
    <SolidColorBrush x:Key="GrayButtonHoverBorder" Color="#71717A" />
    
    <SolidColorBrush x:Key="GrayButtonActiveForeground" Color="#52525B" />
    <SolidColorBrush x:Key="GrayButtonActiveBackground" Color="#C9C9C9" />
    <SolidColorBrush x:Key="GrayButtonActiveBorder" Color="#52525B" />

    <SolidColorBrush x:Key="GrayButtonDisableForeground" Color="#E3E3E3" />
    <SolidColorBrush x:Key="GrayButtonDisableBackground" Color="#E3E3E3" />
    <SolidColorBrush x:Key="GrayButtonDisableBorder" Color="#E3E3E3" />
    <!--  Gray button end  -->


    <!--  Gray TextBox  -->
    <SolidColorBrush x:Key="GrayTextBoxBorder" Color="#BBBBBB" />
    <SolidColorBrush x:Key="GrayTextBoxHoverBorder" Color="#C9C9C9" />
    <!--  Gray TextBox end  -->

    <!--  Gray CheckBox  -->
    <SolidColorBrush x:Key="CheckBoxBorder" Color="#52525B" />
    <SolidColorBrush x:Key="CheckBoxStroke" Color="#FFFFFF" />
    <SolidColorBrush x:Key="CheckBoxBackground" Color="#FFFFFF" />

    <SolidColorBrush x:Key="CheckBoxHoverBorder" Color="#71717A" />
    <SolidColorBrush x:Key="CheckBoxHoverStroke" Color="#FFFFFF" />
    <SolidColorBrush x:Key="CheckBoxHoverBackground" Color="#FFFFFF" />

    <SolidColorBrush x:Key="CheckBoxActiveBorder" Color="Transparent" />
    <SolidColorBrush x:Key="CheckBoxActiveStroke" Color="#FFFFFF" />
    <SolidColorBrush x:Key="CheckBoxActiveBackground" Color="#FF3B82F6" />

    <SolidColorBrush x:Key="CheckBoxDisableBorder" Color="#BBBBBB" />
    <SolidColorBrush x:Key="CheckBoxDisableStroke" Color="#FFFFFF" />
    <SolidColorBrush x:Key="CheckBoxDisableBackground" Color="#BBBBBB" />
    <SolidColorBrush x:Key="CheckBoxDisableForeground" Color="#7D8385" />
    <!--  Gray CheckBox end  -->
    

    <!--  Gray ComboBox  -->
    <Color x:Key="ComboBoxBorderColor">#BBBBBB</Color>
    <Color x:Key="ComboBoxBackgroundColor">#FFFFFF</Color>

    <Color x:Key="ComboBoxHoverBorderColor">#52525B</Color>
    <Color x:Key="ComboBoxHoverBackgroundColor">#BBBBBB</Color>

    <Color x:Key="ComboBoxArrowColor">#BBBBBB</Color>
    <Color x:Key="ComboBoxHoverArrowColor">#7D8385</Color>


    <SolidColorBrush x:Key="ComboBoxForeground" Color="#52525B" />
    <SolidColorBrush x:Key="ComboBoxBackground" Color="#FFFFFF" />
    <SolidColorBrush x:Key="ComboBoxBorder" Color="#BBBBBB" />
    <SolidColorBrush x:Key="ComboBoxArrow" Color="#BBBBBB" />

    <SolidColorBrush x:Key="ComboBoxDisableBackground" Color="#EAEAEA" />
    <SolidColorBrush x:Key="ComboBoxDisableForeground" Color="#BBBBBB" />
    <SolidColorBrush x:Key="ComboBoxDisableBorder" Color="#EAEAEA" />
    <SolidColorBrush x:Key="ComboBoxDisableArrow" Color="#EAEAEA" />

    <SolidColorBrush x:Key="ComboBoxHoverBorder" Color="#52525B" />
    <SolidColorBrush x:Key="ComboBoxHoverBackground" Color="#FFFFFF" />
    <SolidColorBrush x:Key="ComboBoxHoverForeground">#000000</SolidColorBrush>
    <!--  Gray ComboBox  -->

    <!--  RadioButton  -->
    <Color x:Key="RadioButtonCheckMarkColor">#FF3B82F6</Color>
    
    <SolidColorBrush x:Key="RadioButtonBackground" Color="#FFFFFF" />
    <SolidColorBrush x:Key="RadioButtonStroke" Color="#FF3B82F6" />
    <SolidColorBrush x:Key="RadioButtonCheckMark" Color="{StaticResource RadioButtonCheckMarkColor}" />
    
    <SolidColorBrush x:Key="RadioButtonDisableBorder" Color="#EAEAEA" />
    <SolidColorBrush x:Key="RadioButtonDisableBackground" Color="#FFFFFF" />
    <SolidColorBrush x:Key="RadioButtonDisableStroke" Color="#E3E3E3" />

    <SolidColorBrush x:Key="RadioButtonHoverBorderBrush" Color="#FF6798ca" />
    <!--  RadioButton  -->


    <!--  Additional color  -->
    <SolidColorBrush x:Key="ArrowFillColorBrush" Color="#7D8385" />
    <SolidColorBrush x:Key="CheckBoxFocusColorBrush" Color="#60000000" />
    <SolidColorBrush x:Key="AppUpdateProgressBackground" Color="#6E85B7"/>
    <SolidColorBrush x:Key="AppUpdateProgressForeground" Color="#0C356A" />
    <SolidColorBrush x:Key="BodyLightBackground" Color="#FFF2F2F2"/>
    <SolidColorBrush x:Key="BorderLightBackground" Color="#BBBBBB" />
    <SolidColorBrush x:Key="GraphBackground" Color="#FFD2E6FA" />

    <SolidColorBrush x:Key="ListBoxBackground" Color="#FFF0F5FF" />
    <SolidColorBrush x:Key="LableTextBrush" Color="#000000" />
    <SolidColorBrush x:Key="WarningTextBrush" Color="Orange" />
    <SolidColorBrush x:Key="GeneralTextBrush" Color="#52525B" />
</ResourceDictionary>