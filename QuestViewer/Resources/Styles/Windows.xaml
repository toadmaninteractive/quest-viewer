<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="ContentHolderBorder" TargetType="{x:Type Border}">
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Background" Value="{StaticResource WhitePrimaryColorBrush}" />
        <Setter Property="BorderBrush" Value="{StaticResource BorderLightBackground}" />
    </Style>
</ResourceDictionary>